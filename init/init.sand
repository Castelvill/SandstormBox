start init false
    triggers on_boot
    if ([on_boot])
    fun me set_can_be_selected false
    new object "KERNEL" 1 _ _ First
    fun First set_id "KERNEL"
    fun First set_can_be_selected true
    fun First set_can_be_moved_with_mouse true
    fun First enable_selection_border
    //fun First disable_scrolling
    bind First reset
    bind First literal ["scripts/terminal.sand" "scripts/terminalHelp.sand" "scripts/tests.sand"]
    build First true
end
