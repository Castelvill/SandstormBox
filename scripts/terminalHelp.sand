start history
    int [0] i
    value ([v commandHistory size]) commandHistorySize
    -- commandHistorySize
    = program ""
    run printHistory
end

start printHistory true
    if ([i] [commandHistorySize] <)
    string [""] result
    int [0] j
    = j i
    ++ j
    += result j
    += result "\t"
    value ([v commandHistory c i]) nextCommand
    += result nextCommand
    += result "\n"
    ++ i
    run printOnTerminal
end


start wait
    = program "wait"
    int [0] waitingTime
end
start executeWaiting
    triggers each_second
    if ([program] ["wait"] == [second_passed] &&)
    index me [0 0] super_editable_text Terminal
    fun Terminal cut_unprotected_area
    run waitFor0 waitFor1 waitFor2
end
start waitFor0
    if([waitingTime] [0] ==)
    string ["||||||||||||||||||||"] result
    ++ waitingTime
    run printOnTerminalUnprotected
post
    break
end
start waitFor1
    if([waitingTime] [1] ==)
    string ["********************"] result
    ++ waitingTime
    run printOnTerminalUnprotected
post
    break
end
start waitFor2
    if([waitingTime] [2] ==)
    string ["--------------------"] result
    int [0] waitingTime
    run printOnTerminalUnprotected
post
    break
end

